    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            String b=jTextField2.getText();
            String f=jTextField1.getText();
            PreparedStatement query=conn.prepareStatement("insert into fb(b_name, feedback) values(?,?);");
            query.setString(1,b);
            query.setString(2, f);
            JOptionPane.showMessageDialog(this,"THANKYOU FOR THE FEEDBACK");
            search ob=new search();
            ob.setVisible(true);
            this.setVisible(false);
        } catch (SQLException ex) {
            Logger.getLogger(feed.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

