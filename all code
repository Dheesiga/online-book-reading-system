    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            Double i=Double.parseDouble(jTextField1.getText());
            PreparedStatement query1=conn.prepareStatement("delete from author where ISBN=?");
            query1.setDouble(1,i);
            query1.executeUpdate();
            PreparedStatement query2=conn.prepareStatement("delete from publisher where ISBN=?");
            query2.setDouble(1,i);
            query2.executeUpdate();
            PreparedStatement query3=conn.prepareStatement("delete from book where ISBN=?");
            query3.setDouble(1,i);
            query3.executeUpdate();
            JOptionPane.showMessageDialog(this,"DELETED SUCCESSFULLY");
            admin1 ob=new admin1();
            ob.setVisible(true);
            this.setVisible(false);
        } catch (SQLException ex) {
            Logger.getLogger(deleteb.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
