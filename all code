SYSTEM IMPLEMENTATION
I.	TABLE CREATION AND QUERIES:
•	Table creation for table author:
create table author ( a_id char(5) primary key,
		          ISBN double,
		          foreign key (ISBN) references book(ISBN),
		          a_name varchar(50),
		          amail varchar(30));
•	Table creation for table book:
create table book (ISBN double primary key,
		      b_name varchar(100),
		      language varchar(20),
		      genre varchar(20),
		      description varchar(1000),
		      rating double);		
•	Table creation for table fb:
create table fb(b_name varchar(50), 
feedback varchar(100));

•	Table creation for table login:
create table login (log_id char(8) primary key,
		      name varchar(20),
		      password varchar(10),
		      validity date); 
•	Table creation for table publisher:
create table publisher( p_id char(5) primary key,
		          ISBN double,
		          foreign key (ISBN) references book(ISBN),
		          p_name varchar(50),
		          pmail varchar(30));

II.	INTEGRATION OF TABLES AND FRONT END:
//Bookdemo.java
package bookdemo;
import java.util.*;
import java.sql.*;
public class Bookdemo {
static final String DB_URL = "jdbc:mysql://localhost:3306/books";
static final String USER = "root";
static final String PASS = "Shakthii@17";
public static void main(String[] args) {
Connection conn = null;
Statement stmt = null;
try{
Class.forName("com.mysql.jdbc.Driver");
System.out.println("Connecting to database...");
conn = DriverManager.getConnection(DB_URL,USER,PASS);
stmt = conn.createStatement();
Scanner s = new Scanner(System.in);
for(;;){
System.out.println("Click 1 to sign-in 2 to login 3 for admin login 4 to logout:");
int bu=s.nextInt();
if(bu==1){
System.out.println("Signin:");
System.out.println("Enter name:");
String n=s.next();
System.out.println("Enter user name:");
String u=s.next();
System.out.println("Enter password:");
String p=s.next();
System.out.println("Do you want membership? press 1 if needed");
int m=s.nextInt();
if(m==1){
System.out.println("Select your membership validity:\n3 months-Rs.99(1)\n6 months-Rs.170(2)\n1 year-Rs.300(3)");
int ms=s.nextInt();
int v=0;
if (ms==1)
v=3;
else if(ms==2)
v=6;
else if(ms==3)
v=12;
else
System.out.println("Need membership to access");
PreparedStatement query = conn.prepareStatement("insert into login(log_id,name,password)values(?,?,?);");
query.setString(1, u);
query.setString(2, n);
query.setString(3, p);
query.executeUpdate();
PreparedStatement query1 = conn.prepareStatement("update login set validity=adddate(curdate(),"
+ "interval ? month) where name=?");
query1.setInt(1,v);
query1.setString(2, n);
query1.executeUpdate();
PreparedStatement query2 = conn.prepareStatement("select adddate(curdate(), interval ? month) as date");
query2.setInt(1,v);
ResultSet sr=query2.executeQuery();
while(sr.next())
System .out.println("Your membership is valid till "+sr.getDate("date"));
}
else{
System.out.println("Need membership to access");
PreparedStatement query1=conn.prepareStatement("delete from login where log_id=?");
query1.setString(1,u);
query1.executeUpdate();
}
}
else if(bu==3){
System.out.println("login:");
System.out.println("Enter user name:");
String u=s.next();
System.out.println("Enter password:");
String p=s.next();
PreparedStatement query = conn.prepareStatement("Select password from login where log_id=?");
query.setString(1, u);
ResultSet rs1 = query.executeQuery();
String passcheck = new String();
while (rs1.next()) {
passcheck = rs1.getString(1);
}
if (p.equals(passcheck)){
System.out.println("Press 1 to insert, 2 to delete:");
int q=s.nextInt();
if(q==1){
System.out.println("Enter book details");
System.out.println("Enter ISBN number:");
Double i=s.nextDouble();
System.out.println("Enter book name:");
String b=s.next();
System.out.println("Enter book language:");
String l=s.next();
System.out.println("Enter book genre:");
String g=s.next();
System.out.println("Enter book description:");
String d=s.nextLine();
System.out.println("Enter rating:");
Double r=s.nextDouble();
PreparedStatement query1=conn.prepareStatement("insert into book values(?,?,?,?,?,?);");
query1.setDouble(1,i);
query1.setString(2,b);
query1.setString(3,l);
query1.setString(4,g);
query1.setString(5,d);
query1.setDouble(6,r);
query1.executeUpdate();
System.out.println("Enter Author details");
System.out.println("Enter author id:");
String aid=s.next();
System.out.println("Enter author name:");
String an=s.next();
System.out.println("Enter author mail:");
String am=s.next();
PreparedStatement query2=conn.prepareStatement("insert into author values(?,?,?,?);");
query2.setString(1,aid);
query2.setDouble(2,i);
query2.setString(3,an);
query2.setString(4,am);
query2.executeUpdate();
System.out.println("Enter Publisher details");
System.out.println("Enter publisher id:");
String pid=s.next();
System.out.println("Enter publisher name:");
String pn=s.next();
System.out.println("Enter publisher mail:");
String pm=s.next();
PreparedStatement query3=conn.prepareStatement("insert into publisher values(?,?,?,?);");
query3.setString(1,pid);
query3.setDouble(2,i);
query3.setString(3,pn);
query3.setString(4,pm);
query3.executeUpdate();
System.out.println("Inserted success fully");
}
else{
System.out.println("Enter ISBN:");
Double i=s.nextDouble();
PreparedStatement query1=conn.prepareStatement("delete from author where ISBN=?");
query1.setDouble(1,i);
query1.executeUpdate();
PreparedStatement query2=conn.prepareStatement("delete from publisher where ISBN=?");
query2.setDouble(1,i);
query2.executeUpdate();
PreparedStatement query3=conn.prepareStatement("delete from book where ISBN=?");
query3.setDouble(1,i);
query3.executeUpdate();
System.out.println("Deleted success fully");
}
}
else{
System.out.println("Enter correct password");
}
}
else if(bu==2){
System.out.println("login:");
System.out.println("Enter user name:");
String u=s.next();
System.out.println("Enter password:");
String p=s.next();
PreparedStatement query = conn.prepareStatement("Select password from login where log_id=?");
query.setString(1, u);
PreparedStatement sql = conn.prepareStatement("Select datediff(validity,curdate()) as d from login where log_id=?");
sql.setString(1, u);
ResultSet rs1 = query.executeQuery();
String passcheck = new String();
int ex=0;
while (rs1.next()) {
passcheck = rs1.getString(1);
}
if (p.equals(passcheck)){
ResultSet rs2 = sql.executeQuery();
while(rs2.next()){
ex=rs2.getInt(1);
}
if(ex>0){
System.out.println("Your membership is valid for "+ex+" more days");
System.out.println("login successful");
System.out.println("Search a book.....");
String b=s.next();
PreparedStatement mysql = conn.prepareStatement("select exists(select * from book natural join author natural join "
        + "publisher where b_name regexp ?) as c");
mysql.setString(1, b);
ResultSet r = mysql.executeQuery();
int q=0;
while(r.next()){
q=r.getInt("c");
}
if(q==1){
PreparedStatement sql1 = conn.prepareStatement("SELECT * from book natural join author natural join publisher "
+ "where b_name regexp ?"); 
sql1.setString(1,b);
ResultSet rs = sql1.executeQuery();
while(rs.next()){
double ISBN=rs.getDouble("ISBN");
String b_name=rs.getString("b_name");
String language=rs.getString("language");
String genre=rs.getString("genre");
String description=rs.getString("description");
Double rating=rs.getDouble("rating");
String a_name=rs.getString("a_name");
String a_id=rs.getString("a_id");
String a_email=rs.getString("amail");
String p_name=rs.getString("p_name");
String p_id=rs.getString("p_id");
String p_email=rs.getString("pmail");
System.out.println("ISBN: " + ISBN);
System.out.println("Book name: " + b_name);
System.out.println("language: "+language);
System.out.println("genre: "+genre);
System.out.println("Description: "+description);
System.out.println("rating: "+rating);
System.out.println("Author: "+a_name);
System.out.println("Publisher: "+p_name);
System.out.println("\nCan tou give your feedback?(press 1 if read)");
int x= s.nextInt();
if(x==1){
System.out.println("Enter your feedback: ");
s.nextLine();
String y=s.nextLine();
PreparedStatement query1=conn.prepareStatement("insert into fb values(?,?);");
query1.setString(1,b_name);
query1.setString(2,y);
query1.executeUpdate();
System.out.println("Thankyou for your feedback");
}
else{
System.out.println("Enjoy reading....");
}
}
rs.close();
}
else{
System.out.println("book not found");
}
}
else{
System.out.println("your member ship is expired and your account is deleted.... kindly signin again");
PreparedStatement query1=conn.prepareStatement("delete from login where log_id=?");
query1.setString(1,u);
query1.executeUpdate();
}
}
else
System.out.println("your user name or password is incorrect");
}
else{
    break;
}
}
stmt.close();
conn.close();
}catch(SQLException se){
se.printStackTrace();
}catch(Exception e){
e.printStackTrace();
}finally{
try{
if(stmt!=null)
stmt.close();
}catch(SQLException se2){
}
try{
if(conn!=null)
conn.close();
}catch(SQLException se){
se.printStackTrace();
}
}
System.out.println("See you soon!");
}
}
 
