    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            String user=user1.getText();
            String password=pass.getText();
            PreparedStatement query = conn.prepareStatement("select* from login where log_id='"+user+"' and password='"+password+"'");
            ResultSet rs = query.executeQuery();
            PreparedStatement sql = conn.prepareStatement("Select datediff(validity,curdate()) as d from login where log_id=?");
            sql.setString(1, user);
            int ex=0;
            ResultSet rs1 = sql.executeQuery();
            while(rs1.next())
                ex=rs1.getInt(1);
            if(ex<=0){
                JOptionPane.showMessageDialog(this,"VALIDITY EXPIRED");
                PreparedStatement query1=conn.prepareStatement("delete from login where log_id=?");
                query1.setString(1,user);
                query1.executeUpdate();
                JOptionPane.showMessageDialog(this,"RENEW YOUR MEMBERSHIP BY SIGNING IN");
            }
            if(rs.next())
            {
                String s="Your membership is valid for "+ ex +" more days";
                JOptionPane.showMessageDialog(this,s);
                dispose();
                search ob=new search();
                ob.setVisible(true);
                this.setVisible(false);             
            }
            else{
                JOptionPane.showMessageDialog(this,"INVALID USERNAME OR PASSWORD!");
            }
            conn.close();
        } catch (SQLException ex) {
            java.util.logging.Logger.getLogger(loginu.class.getName()).log(java.util.logging.Level.SEVERE, null, ex);
        }
    }                                        

