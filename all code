    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            String u=user.getText();
            String p=pass.getText();
            if(u.equals("admin")){
            PreparedStatement query = conn.prepareStatement("select* from login where log_id='" +u+"' and password='"+p+"'");
            ResultSet rs = query.executeQuery();
            if(rs.next())
            {
                dispose();
                admin1 ob=new admin1();
                ob.setVisible(true);
                this.setVisible(false);
            }
            else{
                JOptionPane.showMessageDialog(this,"INVALID PASSWORD!");
                
            }
            }
            else{
                JOptionPane.showMessageDialog(this,"INVALID USERNAME!");
            }
            conn.close();
        } catch (SQLException ex) {
            java.util.logging.Logger.getLogger(adminlogin.class.getName()).log(java.util.logging.Level.SEVERE, null, ex);
        }
    }                                        

