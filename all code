    }                                           

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            String b=jTextField1.getText();
            if(b.length()>0){
            PreparedStatement sql=conn.prepareStatement("SELECT ISBN,b_name,language,genre,description,rating,a_name,p_name from  book natural join author natural join publisher where b_name = ?");       
            sql.setString(1, b);
            ResultSet rs = sql.executeQuery();
            if(rs.next()){
                String i=String.valueOf(rs.getDouble("ISBN"));
                String bo=rs.getString("b_name");
                String l=rs.getString("language");
                String g=rs.getString("genre");
                String d=rs.getString("description");
                String r=String.valueOf(rs.getDouble("rating"));
                String a=rs.getString("a_name");
                String p=rs.getString("p_name");
                jTextField2.setText(i);
                jTextField3.setText(bo);
                jTextField4.setText(l);
                jTextField5.setText(g);
                jTextField6.setText(d);
                jTextField7.setText(r);
                jTextField8.setText(a);
                jTextField9.setText(p);
            }
            else{
                JOptionPane.showMessageDialog(null,"ENTER CORRECT NAME OF THE BOOK");
                jTextField1.setText("");
            }
            }
            else{
                JOptionPane.showMessageDialog(this,"box is blank");
            }
            
        } catch (SQLException ex) {
            Logger.getLogger(home.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

