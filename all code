    private void subActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
        try{
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/books", "root", "Shakthii@17");
            String name=jTextField1.getText();
            String user=jTextField2.getText();
            String pass=jPasswordField1.getText();
            if(v3.isSelected())
            v=3;
            if(v6.isSelected())
            v=6;
            if(v12.isSelected())
            v=12;
            PreparedStatement query=conn.prepareStatement("insert into login(name,log_id,password,validity)values(?,?,?,adddate(curdate(),interval ? month));");
            query.setString(1,name);
            query.setString(2,user);
            query.setString(3,pass);
            query.setInt(4,v);
            query.executeUpdate();
            JOptionPane.showMessageDialog(this,"SIGN-IN SUCCESSFULLY");
            loginu ob=new loginu();
            ob.setVisible(true);
            this.setVisible(false);
        } catch (SQLException ex) {
            Logger.getLogger(signinu.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                   

